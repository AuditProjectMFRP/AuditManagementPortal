@model Dictionary<string,string>
@{
    ViewData["Title"] = "Questionnaire";
}
<head></head>
<body>
<h4>Questionnaire</h4>
<div >
    <h2>Please Answer the following questions</h2>
</div>
@using (Html.BeginForm("Audit", "Home")) {
    <table class="table">

        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Key</td>
                <td>YES @Html.RadioButton(item.Key,"YES",new {required=""})</td>
                <td>NO @Html.RadioButton(item.Key,"NO", new { required = "" })</td>

            </tr>
        }

    </table>
    <button class="btn btn-lg btn-primary btn-block" type="submit">SUBMIT</button>
}
</body>
